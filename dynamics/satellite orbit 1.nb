Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"fsTrig", "=", 
   RowBox[{"ParallelTable", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       FractionBox[
        RowBox[{
         SqrtBox[
          RowBox[{"1", "-", 
           RowBox[{"e", "^", "2"}]}]], 
         RowBox[{"Sin", "[", 
          RowBox[{"EE", "[", 
           RowBox[{"[", "i", "]"}], "]"}], "]"}]}], 
        RowBox[{"1", "-", 
         RowBox[{"e", " ", 
          RowBox[{"Cos", "[", 
           RowBox[{"EE", "[", 
            RowBox[{"[", "i", "]"}], "]"}], "]"}]}]}]], ",", 
       FractionBox[
        RowBox[{
         RowBox[{"Cos", "[", 
          RowBox[{"EE", "[", 
           RowBox[{"[", "i", "]"}], "]"}], "]"}], "-", "e"}], 
        RowBox[{"1", "-", 
         RowBox[{"e", " ", 
          RowBox[{"Cos", "[", 
           RowBox[{"EE", "[", 
            RowBox[{"[", "i", "]"}], "]"}], "]"}]}]}]]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "1", ",", 
       RowBox[{"Npoints", "+", "1"}]}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"A", "=", 
   FractionBox[
    RowBox[{"n", " ", "a"}], 
    SqrtBox[
     RowBox[{"1", "-", 
      SuperscriptBox["e", "2"]}]]]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"speeds", "=", 
   RowBox[{"ParallelTable", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{"-", "A"}], " ", 
        RowBox[{"fsTrig", "[", 
         RowBox[{"[", 
          RowBox[{"i", ",", "1"}], "]"}], "]"}]}], ",", 
       RowBox[{"A", 
        RowBox[{"(", 
         RowBox[{"e", "+", 
          RowBox[{"fsTrig", "[", 
           RowBox[{"[", 
            RowBox[{"i", ",", "2"}], "]"}], "]"}]}], ")"}]}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "1", ",", 
       RowBox[{"Npoints", "+", "1"}]}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"newspeeds", "=", 
   RowBox[{"Transpose", "[", 
    RowBox[{"turn", ".", 
     RowBox[{"Transpose", "[", "speeds", "]"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"ListPlot", "[", 
  RowBox[{
   RowBox[{"Transpose", "@", 
    RowBox[{"{", 
     RowBox[{"ts", ",", 
      RowBox[{"newspeeds", "[", 
       RowBox[{"[", 
        RowBox[{"All", ",", "1"}], "]"}], "]"}]}], "}"}]}], ",", 
   RowBox[{"PlotLabel", "\[Rule]", 
    RowBox[{"Style", "[", 
